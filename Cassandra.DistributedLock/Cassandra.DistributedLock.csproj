<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net471</TargetFramework>
    <AssemblyName>SkbKontur.Cassandra.DistributedLock</AssemblyName>
    <RootNamespace>SKBKontur.Catalogue.CassandraPrimitives.RemoteLock</RootNamespace>
    <PackageId>$(AssemblyName)</PackageId>
    <VersionPrefix>2.0.0</VersionPrefix>
    <Version>$(VersionPrefix)-rc6</Version>
    <AssemblyVersion>$(VersionPrefix).0</AssemblyVersion>
    <Authors>Ivan Burmistrov, Fedor Fominykh, Andrew Kostousov</Authors>
    <PackageTags>cassandra distributed lock</PackageTags>
    <Description>.NET library implementing distributed lock synchronization primitive using Apache Cassandra</Description>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/skbkontur/cassandra-distributed-lock</RepositoryUrl>
    <PackageProjectUrl>$(RepositoryUrl)</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GroBuf" Version="1.3.0" />
    <PackageReference Include="JetBrains.Annotations" Version="11.1.0" PrivateAssets="All" />
    <PackageReference Include="SkbKontur.Cassandra.ThriftClient" Version="2.0.0-rc6" />
  </ItemGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>$(MSBuildProjectName).Tests</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>

</Project>
